// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Usuario {
rut String @id
nombres String
apellidos String
correo String
clave String
area String
cargo String
tipHorario String
empresa String
asistencia DateTime?
qrCode String
pin Int
}

