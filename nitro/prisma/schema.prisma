// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  rut           String  @id
  nombres       String
  apellidos     String
  correo        String
  clave         String
  area          String
  cargo         String
  tipoHorario   String
  empresa       String
  qrCode        String
  pin           Int
}